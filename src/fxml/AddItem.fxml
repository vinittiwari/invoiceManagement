   <children>
   	<Text fx:id="welcomeText" text="home" GridPane.columnIndex="1" GridPane.columnSpan="2" />
   	<Label text="Item Name:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
   	<TextField fx:id="item_name" prefHeight="25.0" prefWidth="274.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   	<Label text="Price:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
   	<TextField fx:id="price" prefWidth="150" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   	<Label text="Item Code" GridPane.columnIndex="0" GridPane.rowIndex="4" />
   	<TextField fx:id="item_code" prefWidth="150" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   	<Label text="Unit" GridPane.columnIndex="0" GridPane.rowIndex="6" />
   	<TextField fx:id="unit" prefWidth="150" GridPane.columnIndex="1" GridPane.rowIndex="6" />
   	<Label text="Cess" GridPane.columnIndex="0" GridPane.rowIndex="7" />
   	<TextField fx:id="cess" prefWidth="150" GridPane.columnIndex="1" GridPane.rowIndex="7" />
   	
   		
   	<HBox alignment="BOTTOM_CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="12">
      <children>
      		<Button fx:id="addItemSubmit" onAction="#handleAddItem" text="Add Item" />
      </children>
   	</HBox>
      <Button mnemonicParsing="false" onAction="#handleBackToHome" prefHeight="25.0" prefWidth="99.0" text="Back To Home" />
      <Label text="Rate" GridPane.rowIndex="8" />
      <TextField fx:id="rate" prefWidth="150" GridPane.columnIndex="1" GridPane.rowIndex="8" />
      <Label fx:id="item_name_valid" prefHeight="17.0" prefWidth="55.0" text="*invalid" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Label fx:id="price_valid" prefHeight="17.0" prefWidth="55.0" text="*invalid" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <Label fx:id="item_code_valid" prefHeight="17.0" prefWidth="55.0" text="*invalid" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <Label fx:id="unit_valid" prefHeight="17.0" prefWidth="55.0" text="*invalid" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="6" />
      <Label fx:id="cess_valid" prefHeight="17.0" prefWidth="55.0" text="*invalid" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="7" />
      <Label fx:id="rate_valid" prefHeight="17.0" prefWidth="55.0" text="*invalid" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="8" />
