<GridPane alignment="TOP_LEFT" hgap="10.0" prefHeight="659.0" prefWidth="618.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<padding>
		<Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
